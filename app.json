  "repository": "https://github.com/augustogoncalves/forge-3leg.nodejs-template",
  "logo": "https://avatars0.githubusercontent.com/u/8017462?v=3&s=200",
  "keywords": [
    "express",
    "framework",
    "autodesk",
    "forge",
    "template"
